{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "extends": ["plugin:vue/vue3-recommended", "airbnb-base", "prettier", "prettier/vue"],
  "plugins": ["vue", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "vue/no-unused-vars": "off",
    "no-unused-vars": "off",
    "vue/max-attributes-per-line": [ "warn", { "singleline": 5 }]
  }
}
